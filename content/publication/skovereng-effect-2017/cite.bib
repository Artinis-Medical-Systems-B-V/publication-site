 month = {jan},
 number = {1},
 pages = {e0169573},
 pmid = {28060894},
 title = {The effect of cadence on shank muscle oxygen consumption and deoxygenation in relation to joint specific power and cycling kinematics},
 url = {http://dx.plos.org/10.1371/journal.pone.0169573},
 volume = {12},
