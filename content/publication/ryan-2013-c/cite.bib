 number = {2},
 pages = {230--237},
 pmid = {23154991},
 title = {A comparison of exercise type and intensity on the noninvasive assessment of skeletal muscle mitochondrial function using near-infrared spectroscopy},
 volume = {114},
 year = {2013}
